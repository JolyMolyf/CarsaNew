  "engines": {
    "node": ">=14.17.6",
    "npm": ">=6.14.15"
  },
  "scripts": {
    "start": "tsnd --respawn index.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "migration:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all --seeders-path database/seeders --config config/db.js --debug",
    "seed:undo": "npx sequelize-cli db:seed:undo:all --seeders-path database/seeders --config config/db.js --debug",
    "test": "jest -c ./jest.unit.json --detectOpenHandles",
    "test:coverage": "jest --coverage -c ./jest.unit.json",
    "lint": "eslint **/*.ts",
    "format": "eslint **/*.ts --fix"
  },
  "keywords": [],
